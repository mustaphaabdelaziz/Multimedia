                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font name="Ubuntu Condensed" size="19.0" />
                     </font>
                  </JFXButton>
                  <JFXButton fx:id="Zoom_In" buttonType="RAISED" contentDisplay="CENTER" disable="true" graphicTextGap="0.0" onAction="#zoomIn" prefHeight="42.0" prefWidth="109.0" text="Zoom In">
